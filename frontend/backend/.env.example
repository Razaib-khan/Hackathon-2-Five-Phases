# Database
DATABASE_URL="postgresql://username:password@localhost:5432/aido_backend"

# JWT Configuration
JWT_SECRET="your-super-secret-jwt-secret-key"
JWT_REFRESH_SECRET="your-super-secret-jwt-refresh-secret-key"
JWT_ACCESS_EXPIRES_IN="15m"
JWT_REFRESH_EXPIRES_IN="7d"

# Server Configuration
PORT=8000
NODE_ENV=development

# API Configuration
API_VERSION=v1
BASE_URL=http://localhost:8000

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info